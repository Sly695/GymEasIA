{
  "expo": {
    "name": "GymEasIA",
    "slug": "gymeasia",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gymeasia.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.gymeasia.app",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow GymEasIA to access your camera to record workout videos."
        }
      ]
    ]
  }
}
